= form_for @lifestyle_cue_inference_connector do |f|
  - if @lifestyle_cue_inference_connector.errors.any?
    #error_explanation
      %h2= "#{pluralize(@lifestyle_cue_inference_connector.errors.count, "error")} prohibited this lifestyle_cue_inference_connector from being saved:"
      %ul
        - @lifestyle_cue_inference_connector.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :inference_id
    = f.number_field :inference_id

  .field
    = f.label :lifestyle_cue_id
    = f.number_field :lifestyle_cue_id

  .actions
    = f.submit 'Save'
