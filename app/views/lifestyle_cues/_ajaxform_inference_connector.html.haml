= form_for([@lifestyle_cue_inference_connector], remote: true) do |f|
  - if @lifestyle_cue_inference_connector.errors.any?
    #error_explanation
      %h2= "#{pluralize(@lifestyle_cue_inference_connector.errors.count, "error")} prohibited this lifestyle_cue from being saved:"
      %ul
        - @lifestyle_cue_inference_connector.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :cue
    = f.text_field :cue
  .actions
    = f.submit 'Save'
